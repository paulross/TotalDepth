          <Value>8</Value>
        </ColorEntry>
        <ColorEntry>
          <Color>ff00ff</Color>
          <GrayColor>5f5f5f</GrayColor>
          <Value>12</Value>
        </ColorEntry>
        <ColorEntry>
          <Color>ff00bf</Color>
          <GrayColor>5f5f5f</GrayColor>
          <Value>17</Value>
        </ColorEntry>
        <ColorEntry>
          <Color>ff0080</Color>
          <GrayColor>4f4f4f</GrayColor>
          <Value>25</Value>
