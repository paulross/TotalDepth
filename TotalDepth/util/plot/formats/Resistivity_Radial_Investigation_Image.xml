      <ChannelName>AHTRI</ChannelName>
      <LeftLimit>0</LeftLimit>
      <RightLimit>179</RightLimit>
      <LgColorTable UniqueId="ColorTable">
        <ColorEntry>
          <Color>00ff00</Color>
          <GrayColor>ffffff</GrayColor>
          <Value>Absent</Value>
        </ColorEntry>
        <ColorEntry>
          <Color>bfffff</Color>
